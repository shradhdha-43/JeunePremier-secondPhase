              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            currencyCode: '{{ shop.currency }}'
          });
        }
      }


      znDutyTaxEstimateOnDetailPage();
      znDutyTaxEstimateOnCartPage();
