    {% if template == '404' %}  
    <script>
      var getLoc = window.location.href;
      if(getLoc.includes("cartopen=true")) {
        window.location.replace(`https://{{ shop.domain }}{{ routes.root_url }}?cartopen=true`);
      }
    </script>
    {% endif %}
    {% if template != 'cart' %}  
    <script>
      var loc = window.location.href;
      localStorage.setItem("PrevLocation" , loc);
    </script>
    {% endif %}
    {% if template == 'cart' %}  
    <script>
      var getLoc = localStorage.getItem("PrevLocation");
    	
      if(getLoc == "" || getLoc == null){
      	window.location.replace(`https://{{ shop.domain }}{{ routes.root_url }}?cartopen=true`);
      } else {
        if(getLoc.includes("cartopen=true")) {
          window.location.replace(`${getLoc}`);
        } else {
          if(getLoc.includes("?")) {
            window.location.replace(`${getLoc}&cartopen=true`);
          } else {
            window.location.replace(`${getLoc}?cartopen=true`);
          }
        }
      }      
    </script>
    {% endif %}
    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
